{
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true
  },

 "globals": {
    "expect": true,
    "jest": true
  },

  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "plugins": [
  ],
  "rules": {
    "comma-dangle": 0,
    "no-mixed-spaces-and-tabs": 0,
    "no-alert": 0,
    "import/named": 2,
    "import/default": 2,
    "import/no-unresolved": [ "error" ],
    "no-underscore-dangle": 0,
    "no-case-declarations": 0,
    "strict": 0,
    "no-console": 1,
    "no-trailing-spaces": 0
  }
}
